@model  RDSUX.Models.ProjectDetailsModel
<div class="jumbotron jumbotron-container">
    <p style="color:green;">@ViewBag.result</p>
    <h3 class="create-title">Create Project</h3>
    @using (Html.BeginForm("CreateProject","Home", FormMethod.Post,new { enctype="multipart/form-data"}))
    {
        @Html.AntiForgeryToken()

    <table width="100%">
        <tbody>
            <tr width="100%">
                <td class="form-td">
                    <table class="width100">
                        <tr class="width100">
                            <td align="right" class="listnames">Project Name:</td>
                            <td>
                                @Html.TextBox("ProjectName", Model.ProjectName, new { @class = "form-control" })
                            </td>
                        </tr>
                        <tr>
                            <td>&nbsp;</td>.
                        </tr>
                        <tr class="width100">
                            <td align="right" class="listnames">Project Type:</td>
                            <td>
                                @Html.DropDownList("ProjectTypeModel", Model.ProjectTypeList)
                            </td>
                        </tr>
                        <tr>
                            <td>&nbsp;</td>
                        </tr>
                        <tr class="width100">
                            <td align="right" class="listnames">Scope of Work:</td>
                            <td>
                                @Html.DropDownList("SOWModel", Model.ScopeOfWork)
                            </td>
                        </tr>
                    </table>
                </td>
                <td>
                    <table class="width100">
                        <tr class="width100">
                            <td align="right" class="listnames" valign="middle">Notes:</td>
                            <td>
                                @Html.TextArea("Notes", Model.Notes, 10, 30, new { @class = "form-control" })
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td>&nbsp;</td>
            </tr>
            </tbody>
    </table>
        <table class="width100">

            <tr class="tdbody-bg">

                <td width="30%" class="font12">Job Number:   @Html.TextBox("JobNumber", Model.JobNumber, new { @class = "form-control" })</td>
                <td>&nbsp;</td>
                <td width="30%" class="font12">Job Sheet:<input type="file" name="JobSheet" />
                 </td>
                <td width="30%" >Contract DWGS: <input type="file" name="ContractDWGS" />  </td>
            </tr>
            <tr>
                <td colspan="3">&nbsp;</td>
            </tr>
        </table>

        <table class="table-inner width100">
            <tbody class="tbody-inner">
                <tr class="width100">
                    <td colspan="2">
                        <h4> Standard Information</h4>
                    </td>

                </tr>
                <tr class="width100">
                    <td class="font12">Stock Length :</td>
                    <td class="td-margin">
                        @Html.TextBox("StockLength", Model.BarCode[0].StockLength, new { @class = "form-control" })
                    </td>
                </tr>
                <tr class="width100">
                    <td class="font12">Bar Supports: </td>
                    <td class="td-margin">
                        @Html.TextBox("BarCodeGrade", Model.BarCode[0].BarCodeGrade, new { @class = "form-control" })
                    </td>
                </tr>

                <tr class="width100">
                    <td class="font12">Client Name: </td>
                    <td class="td-margin">
                        @Html.TextBox("StandardSplice", Model.BarCode[0].StandardSplice, new { @class = "form-control" })
                    </td>
                </tr>
                <tr class="width100">
                    <td class="font12">Type of Couplers: </td>
                    <td class="td-margin">
                        @Html.TextBox("MachanicSplice", Model.BarCode[0].MachanicSplice, new { @class = "form-control" })
                    </td>
                </tr>
                <tr width="100%">
                    <td colspan="2">&nbsp;</td>
                </tr>
                </tbody>
        </table>

        @*<table width="100%">
            <tr>
                <td>&nbsp;</td>
            </tr>
            <tr>
                <td width="30%" class="font12">Contract DWGS: <input type="file" name="ContractDWGS" /> </td>
            </tr>
        </table>*@
        <div align="right">
            <input type="submit" class="btn btn-default create-btn" value="CreateProject" 
                   onclick="location.href='@Url.Action("Home", "Home11")'"/>
        </div>

    }
    </div>
<div>
    @Html.ActionLink("Back to Projects", "ProjectList")
</div>